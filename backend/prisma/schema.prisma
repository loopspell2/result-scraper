generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Result{
  rollNo      String    @unique
  name        String
  cgpa        String
  resultDes   String
}

model EnrollmentNo{
  rollNo      String    @unique
  success     Boolean   @default(false)
  picked      Boolean   @default(false)
  notFound    Boolean   @default(false)
}